{
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "jsx-a11y/href-no-hash": [0],
      "radix": [0],
      "import/no-extraneous-dependencies": [
        0
      ]
    }
  }